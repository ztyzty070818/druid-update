			, open_store_order.coupon_price AS open_store_order__coupon_price, open_store_order.verify_status AS open_store_order__verify_status, open_store_order.subject AS open_store_order__subject, open_store_order.body AS open_store_order__body, open_store_order.detail AS open_store_order__detail \
			, open_store_order.express_type AS open_store_order__express_type, open_store_order.express_name AS open_store_order__express_name, open_store_order.express_code AS open_store_order__express_code, open_store_order.express_time AS open_store_order__express_time, open_store_order.express_area AS open_store_order__express_area \
			, open_store_order.express_phone AS open_store_order__express_phone, open_store_order.pay_status AS open_store_order__pay_status, open_store_order.pay_channel AS open_store_order__pay_channel, open_store_order.send_status AS open_store_order__send_status, open_store_order.send_time AS open_store_order__send_time \
			, open_store_order.refund_time AS open_store_order__refund_time, open_store_order.refund_status AS open_store_order__refund_status, open_store_order.refund_approval AS open_store_order__refund_approval, open_store_order.refund_comment AS open_store_order__refund_comment, open_store_order.refund_remark AS open_store_order__refund_remark \
			, open_store_order.result_status AS open_store_order__result_status, open_store_order.same_way AS open_store_order__same_way, open_store_order.open_refund_id AS open_store_order__open_refund_id, open_store_order.create_time AS open_store_order__create_time, open_store_order.invalid_time AS open_store_order__invalid_time \
			, open_store_order.confirm_time AS open_store_order__confirm_time, open_store_order.resource_type AS open_store_order__resource_type, open_store_order.coupon_record_id AS open_store_order__coupon_record_id, open_store_order.type AS open_store_order__type, open_store_order.xfsj_id AS open_store_order__xfsj_id \
			, open_store_order.remark AS open_store_order__remark \
		FROM open_order \
			LEFT JOIN open_store_order ON open_order.out_trade_no = open_store_order.out_trade_no \
		UNION \
		SELECT open_order.id AS open_order__id, open_order.total_id AS open_order__total_id, open_order.account_id AS open_order__account_id, open_order.community_id AS open_order__community_id, open_order.app_id AS open_order__app_id \
			, open_order.service_id AS open_order__service_id, open_order.uid AS open_order__uid, open_order.caution_id AS open_order__caution_id, open_order.pay_channel AS open_order__pay_channel, open_order.out_trade_no AS open_order__out_trade_no \
			, open_order.client_ip AS open_order__client_ip, open_order.project_type AS open_order__project_type, open_order.trade_type AS open_order__trade_type, open_order.weixin_open_id AS open_order__weixin_open_id, open_order.expire_time AS open_order__expire_time \
			, open_order.bussiness_type AS open_order__bussiness_type, open_order.create_time AS open_order__create_time, open_order.update_time AS open_order__update_time, open_order.bill_status AS open_order__bill_status, open_order.bill_id AS open_order__bill_id \
			, open_order.bill_create_time AS open_order__bill_create_time, open_order.community_trade_no AS open_order__community_trade_no, open_order.bill_finished_time AS open_order__bill_finished_time, open_order.total_amount AS open_order__total_amount, open_order.platform_charge AS open_order__platform_charge \
			, open_order.platform_charge_rate AS open_order__platform_charge_rate, open_order.service_charge AS open_order__service_charge, open_order.service_charge_rate AS open_order__service_charge_rate, open_order.receipt_amount AS open_order__receipt_amount, open_order.tixian_thrid AS open_order__tixian_thrid \
			, open_order.tixian_community AS open_order__tixian_community, open_order.refund_status AS open_order__refund_status, open_order.refund_reason AS open_order__refund_reason, open_order.refund_id AS open_order__refund_id, open_order.refund_remark AS open_order__refund_remark \
			, open_order.refund_create_time AS open_order__refund_create_time, open_order.refund_money AS open_order__refund_money, open_order.refund_bill_id AS open_order__refund_bill_id, open_order.refund_account_id AS open_order__refund_account_id, open_order.same_way AS open_order__same_way \
			, open_order.withdraw_status AS open_order__withdraw_status, open_order.type AS open_order__type, open_order.service_charge_status AS open_order__service_charge_status, open_order.balance_status AS open_order__balance_status, open_order.service_charge_bill_id AS open_order__service_charge_bill_id \
			, open_order.service_account AS open_order__service_account, open_order.service_account_mchid AS open_order__service_account_mchid, open_order.merchant_bill_id AS open_order__merchant_bill_id, open_order.bill_split_status AS open_order__bill_split_status, open_order.bill_split_time AS open_order__bill_split_time \
			, open_order.split_amount AS open_order__split_amount, open_order.open_id AS open_order__open_id, open_order.out_status AS open_order__out_status, open_order.out_status_content AS open_order__out_status_content, open_order.settle_id AS open_order__settle_id \
			, open_order.pf_balance_status AS open_order__pf_balance_status, open_order.pf_bussiness_balance_status AS open_order__pf_bussiness_balance_status, open_order.pf_refund_status AS open_order__pf_refund_status, open_order.pf_refund_time AS open_order__pf_refund_time, open_order.subject AS open_order__subject \
			, open_order.body AS open_order__body, open_order.detail AS open_order__detail, open_order.goods_detail AS open_order__goods_detail, open_store_order.id AS open_store_order__id, open_store_order.user_id AS open_store_order__user_id \
			, open_store_order.open_id AS open_store_order__open_id, open_store_order.community_id AS open_store_order__community_id, open_store_order.service_id AS open_store_order__service_id, open_store_order.addr_info_id AS open_store_order__addr_info_id, open_store_order.addr_info AS open_store_order__addr_info \
			, open_store_order.addr_detail AS open_store_order__addr_detail, open_store_order.user_name AS open_store_order__user_name, open_store_order.mobile AS open_store_order__mobile, open_store_order.order_code AS open_store_order__order_code, open_store_order.out_trade_no AS open_store_order__out_trade_no \
			, open_store_order.express_money AS open_store_order__express_money, open_store_order.goods_total AS open_store_order__goods_total, open_store_order.original_price AS open_store_order__original_price, open_store_order.discount_price AS open_store_order__discount_price, open_store_order.total_amount AS open_store_order__total_amount \
			, open_store_order.coupon_price AS open_store_order__coupon_price, open_store_order.verify_status AS open_store_order__verify_status, open_store_order.subject AS open_store_order__subject, open_store_order.body AS open_store_order__body, open_store_order.detail AS open_store_order__detail \
			, open_store_order.express_type AS open_store_order__express_type, open_store_order.express_name AS open_store_order__express_name, open_store_order.express_code AS open_store_order__express_code, open_store_order.express_time AS open_store_order__express_time, open_store_order.express_area AS open_store_order__express_area \
			, open_store_order.express_phone AS open_store_order__express_phone, open_store_order.pay_status AS open_store_order__pay_status, open_store_order.pay_channel AS open_store_order__pay_channel, open_store_order.send_status AS open_store_order__send_status, open_store_order.send_time AS open_store_order__send_time \
			, open_store_order.refund_time AS open_store_order__refund_time, open_store_order.refund_status AS open_store_order__refund_status, open_store_order.refund_approval AS open_store_order__refund_approval, open_store_order.refund_comment AS open_store_order__refund_comment, open_store_order.refund_remark AS open_store_order__refund_remark \
			, open_store_order.result_status AS open_store_order__result_status, open_store_order.same_way AS open_store_order__same_way, open_store_order.open_refund_id AS open_store_order__open_refund_id, open_store_order.create_time AS open_store_order__create_time, open_store_order.invalid_time AS open_store_order__invalid_time \
			, open_store_order.confirm_time AS open_store_order__confirm_time, open_store_order.resource_type AS open_store_order__resource_type, open_store_order.coupon_record_id AS open_store_order__coupon_record_id, open_store_order.type AS open_store_order__type, open_store_order.xfsj_id AS open_store_order__xfsj_id \
			, open_store_order.remark AS open_store_order__remark \
		FROM open_order \
			RIGHT JOIN open_store_order ON open_order.out_trade_no = open_store_order.out_trade_no \
	) b \
		RIGHT JOIN open_quick_order ON b.open_order__out_trade_no = open_quick_order.out_trade_no \
		LEFT JOIN community_service ON open_quick_order.service_id = community_service.id \
		LEFT JOIN open_merchant_info ON community_service.merchant_id = open_merchant_info.id \
		LEFT JOIN community_service_category ON community_service.category_id = community_service_category.id \
		LEFT JOIN open_service_detail ON open_quick_order.service_id = open_service_detail.service_id \
		LEFT JOIN community ON open_quick_order.community_id = community.id \
		LEFT JOIN area ON community.area_id = area.id \
) order_info \
	RIGHT JOIN open_store_good_order_rel ON open_store_order__id = open_store_good_order_rel.order_id \
	RIGHT JOIN open_store_good ON open_store_good_order_rel.good_id = open_store_good.id ) J  \
where \$CONDITIONS AND \
open_order__update_time BETWEEN '$dateStr' AND '$todayStr' \
OR open_store_order__send_time BETWEEN '$dateStr' AND '$todayStr' \
OR open_store_order__refund_time BETWEEN '$dateStr' AND '$todayStr' \
OR open_store_order__confirm_time BETWEEN '$dateStr' AND '$todayStr' \
OR open_store_good__update_time BETWEEN '$dateStr' AND '$todayStr' \
"