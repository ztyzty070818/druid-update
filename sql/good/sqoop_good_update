			, area.lng AS area__lng, area.lat AS area__lat, area.pinyin AS area__pinyin \
		FROM open_order \
			RIGHT JOIN open_store_order ON open_order.out_trade_no = open_store_order.out_trade_no \
			LEFT JOIN community_service ON open_store_order.service_id = community_service.id \
			LEFT JOIN open_merchant_info ON community_service.merchant_id = open_merchant_info.id \
			LEFT JOIN community_service_category ON community_service.category_id = community_service_category.id \
			LEFT JOIN open_service_detail ON open_store_order.service_id = open_service_detail.service_id \
			LEFT JOIN community ON open_store_order.community_id = community.id \
			LEFT JOIN area ON community.area_id = area.id \
	) a \
		LEFT JOIN open_quick_order ON a.open_order__out_trade_no = open_quick_order.out_trade_no \
	UNION \
